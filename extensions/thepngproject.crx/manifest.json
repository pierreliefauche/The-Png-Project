{
  "name": "The Png Project",
  "version": "3.0.1",
  "manifest_version": 2,
  "description": "Download PNG versions of icons on thenounproject.com",
  "icons": { "16": "icon16.png",
             "48": "icon48.png",
            "128": "icon128.png" },
  "homepage_url": "http://thepngproject.com/",
	"content_scripts": [
	    {
	      "matches": ["http://thenounproject.com/*"],
	      "js": ["jquery.js", "extension.js"],
				"run_at": "document_end"
	    }
	  ]
}
